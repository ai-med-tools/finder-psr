{
    "extends": "@tsconfig/node18/tsconfig.json",
    "compilerOptions": {
        "rootDir": "./src",
        "outDir": "./build",
        "declaration": true,
        "downlevelIteration": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "lib": ["es2021"]
    }
}
